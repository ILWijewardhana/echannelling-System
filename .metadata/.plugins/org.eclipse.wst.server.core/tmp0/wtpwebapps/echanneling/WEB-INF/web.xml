<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
         <servlet>
			<servlet-name>dlogin</servlet-name>
			<servlet-class>com.doctor.dloginServelet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>dlogin</servlet-name>
			<url-pattern>/dlog</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>doctorSignup</servlet-name>
			<servlet-class>com.doctor.doctorSignup</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>doctorSignup</servlet-name>
			<url-pattern>/insert</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>updateDoctor</servlet-name>
			<servlet-class>com.doctor.updateDocServelet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>updateDoctor</servlet-name>
			<url-pattern>/update</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>deleteDoctor</servlet-name>
			<servlet-class>com.doctor.DeleteDoctorServelet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deleteDoctor</servlet-name>
			<url-pattern>/delete</url-pattern>
		</servlet-mapping>
		
	
         
</web-app>